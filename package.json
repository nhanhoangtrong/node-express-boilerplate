{
    "name": "node-express-boilerplate",
    "version": "0.1.0",
    "description": "A ExpressJS and Node v8 boilerplate for fast web development.",
    "main": "index.js",
    "repository": "git@github.com:nhanhoangtrong/node-express-boilerplate.git",
    "author": "Nhan Hoang <siege250294@gmail.com>",
    "license": "MIT",
    "private": false,
    "scripts": {
        "start": "cross-env NODE_ENV=production node index.js",
        "dev": "cross-env NODE_ENV=development nodemon index.js --ext \"js json\" --watch . --verbose",
        "test": "jest --forceExit",
        "lint": "eslint .",
        "coverage": "jest --coverage",
        "prettier": "prettier --write \"{app,__{tests,mocks}__}/**/*.js\""
    },
    "dependencies": {

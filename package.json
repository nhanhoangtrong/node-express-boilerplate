        "cookie-parser": "^1.4.3",
        "express": "^4.16.2",
        "express-flash": "^0.0.2",
        "express-hbs": "^1.0.4",
        "express-session": "^1.15.6",
        "lusca": "^1.5.2",
        "morgan": "^1.9.0",
        "multer": "^1.3.0",
        "nconf": "^0.10.0",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "redis": "^2.8.0",
        "winston": "^3.0.0-rc1"
    },
    "devDependencies": {
        "cross-env": "^5.1.1",
        "eslint": "^4.13.0",
        "jest": "^22.4.3",
        "nodemon": "^1.12.5",
        "prettier": "^1.9.2",
        "supertest": "^3.0.0"
    }
